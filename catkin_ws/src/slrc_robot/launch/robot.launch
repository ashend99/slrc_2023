<?xml version="1.0"?>

<launch>
  <env name="ROSCONSOLE_CONFIG_FILE" value="$(find webots_ros)/config/rosconsole.config"/>
  
  <!-- start Webots -->
  <include file="$(find webots_ros)/launch/webots.launch">
    <arg name="world" value="$(find slrc_robot)/webots_project/worlds/arena.wbt"/>
    <arg name="mode" value="pause"/>
    <arg name="no_gui" value="false"/>    
    <arg name="stream" default="false"/>
  </include>

  <arg name="duration" default="20" doc="Duration in seconds"/>
  <arg name="auto_close" default="false" doc="Startup mode"/>
  <node name="epuck_line" pkg="webots_ros" type="e_puck_line" args="$(arg duration)" required="$(arg auto_close)" output="screen"/>



  
</launch>
